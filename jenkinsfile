pipeline {
  agent any
  stages {
    stage('Build'){
	  steps{
	     println('This is Build stage');
	  }
	}
   stage('Test'){
     steps{
	   println('This is Test stage');
	 } 
	}
	stage('Deploy'){
	  steps{
	    println('This is Deploy stage');
	    //sh "echo $PATH"
	    sh "dos2unix launch.sh"
	    sh "chmod 775 launch.sh"
	    sh "launch.sh"
	  }
	}
 }
}
